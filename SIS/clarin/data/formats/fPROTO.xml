<?xml-model href="../../schemas/format.xsd" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<format xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fPROTO" xsi:noNamespaceSchemaLocation="../../schemas/format.xsd" display="show">
  <titleStmt>
    <title>Protocol Buffers (ProtoBuf)</title>
    <abbr>ProtoBuf</abbr>
  </titleStmt>
  <keyword>interoperability</keyword>
  <keyword>structured data</keyword>
  <keyword>serialization</keyword>
  <info type="description">
   <p>
    Protocol Buffers (ProtoBuf) is a language-neutral, platform-neutral, extensible mechanism for serializing structured data.</p>
    <p>Visit:
      <a href="https://developers.google.com/protocol-buffers">Protocol Buffers - Google Developers</a> for more details.</p>
  </info>
  <mimeType>application/x-protobuf</mimeType>
  <fileExt recommended="yes">.proto</fileExt>
</format>